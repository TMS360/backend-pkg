"""Результат пагинации"""
type Pagination {
    """Текущая страница"""
    page: Int!
    """Записей на странице"""
    limit: Int!
    """Всего записей"""
    total: Int!
    """Всего страниц"""
    totalPages: Int!
}

"""Входные параметры пагинации"""
input PaginationInput {
    """Номер страницы (с 1)"""
    page: Int!
    """Записей на странице (макс 100)"""
    limit: Int!
}